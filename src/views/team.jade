  mixin member-card(member)
    .col-lg-3.col-md-6.col-6
      .card.card-team
        img.img-fluid(src=asset_url + "images/" + member.profilePic alt=member.name)
        .card-body
          .person-info.-nerds
            h5= member.name
          .professional-platforms
            if member.githubId
              a(href="https://github.com/" + member.githubId, target="_blank")
                i.icon-github

block content
  section.section-team.section-banner
    .heading-overlay
      h1.section-heading Josh Leadership
    a.scroll-down(href="#ourTeam")
      i.icon-angle-double-down
  section#ourTeam.section-content.section-leadership
    .container
      .section-header
        h2.section-heading Josh Leadership
        p Re-engineering existing inefficient architectures with zero down-time and complete data integrity
      .row
        for leader in leaders
          +leader-card(leader)

  section.section-content.section-nerds
    .container
      h1.section-heading Meet Our Nerds
      .row
        for member in members
          +member-card(member)

      .btn-wrap
        a.btn.btn-gray(href="about-us.html") About Us
        a.btn.btn-outline-gray(href="http://careers.joshsoftware.com/") Career

block append scripts
  script.
    $(document).ready(function () {
      $('html, body').animate({
        scrollTop: $('#ourTeam').offset().top
      }, 'slow');
    });
